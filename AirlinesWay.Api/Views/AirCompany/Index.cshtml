@model AirlinesWay.Application.Models.AirCompanyResponseModel

@{
    ViewBag.Title = "title";
}

@using (Html.BeginForm("AddAirCompany", "AirCompany", FormMethod.Post, new {id="frmRegister"})) {

    @Html.LabelFor(m => m.Name)
    @Html.TextBoxFor(m => m.Name)

    @Html.ListBoxFor(m => m.AirLineIds, Model.Airlines, "Select AirLines for AirCompany")

    <input type='Submit' value='Register' id= "btnRegister" />
}
